{
  "name": "Bookmarks Baby!",
  "version": "0.0.0",
  "manifest_version": 2,
  "description": "Bookmarks management for elite babies.",
  "homepage_url": "http://ngryman.sh",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "bg/background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "page_action/page_action.html"
  },
  "chrome_url_overrides": {
    "bookmarks": "overrides/bookmarks/index.html"
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "storage",
    "chrome://favicon/*"
  ],
  "omnibox": {
    "keyword": "bb"
  }
}
